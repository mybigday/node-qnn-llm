{
  "name": "node-qnn-llm",
  "version": "1.1.0",
  "description": "QNN LLM binding for Node.js",
  "main": "index.js",
  "repository": "https://github.com/mybigday/node-qnn-llm",
  "author": "Hans <hans.chen@bricks.tools>",
  "license": "MIT",
  "optionalDependencies": {
    "node-qnn-llm-linux-arm64": "1.1.0",
    "node-qnn-llm-win32-arm64": "1.1.0",
    "node-qnn-llm-linux-x64": "1.1.0",
    "node-qnn-llm-win32-x64": "1.1.0"
  },
  "devDependencies": {
    "cmake-js": "^7.3.0",
    "node-addon-api": "^8.0.0",
    "release-it": "^19.0.4"
  },
  "scripts": {
    "build": "cmake-js build",
